{
    "Logging": {
        "LogLevel": {
            "Default": "Error",
            "Microsoft": "Error",
            "Microsoft.Hosting.Lifetime": "Error"
        }
    },
    "ConnectionStrings": {
        "RulesExecutorStore": "Data Source=localhost; Database=DataChecker; Integrated Security=true;"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning",
                "Hangfire": "Information",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "Enrich": [
            "FromLogContext"
        ],
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "c:/tmp/logs/jobexecutordaemon-.log",
                    "shared": "true",
                    "rollingInterval": "Day"
                }
            }
        ]
    },
    "JobExecutor": {
        "Processes" : 5
    }
}
