{
    "ConnectionStrings": {
        "DataCheckerStore": "Data Source=localhost; Database=DataChecker; Integrated Security=true;",
        "RulesExecutorStore": "Data Source=localhost; Database=DataChecker; Integrated Security=true;"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning",
                "Hangfire": "Information"
            }
        },
        "Enrich": [
            "FromLogContext"
        ],
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "c:/tmp/logs/datachecker-.log",
                    "shared": "true",
                    "rollingInterval": "Day"
                }
            }
        ]
    },
    "MaxNumberResults": 100,
    "JobExecutor" : {
        "Host" : false,
        "Processes" : 2
    }
}
